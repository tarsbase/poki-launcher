desktop_file = {
    SOI ~
    ((header | param)? ~ NEWLINE)* ~
    EOI
}

header = { "[" ~ header_val ~ "]" }
header_val = { (!("[" | "]") ~ ANY)* }

param = { key ~ "=" ~ value}
key = { ASCII_ALPHANUMERIC+ }
value = { (!NEWLINE ~ ANY)* }

